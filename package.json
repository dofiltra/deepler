    "@types/make-fetch-happen": "^9.0.1",
    "@types/node": "^17.0.9",
    "@types/tunnel": "^0.0.3",
    "prettier": "^2.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.3"
  },
